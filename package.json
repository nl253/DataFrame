{
  "name": "dataf",
  "version": "0.2.1",
  "description": "Dataframe & Series library for statistics and tabular data manipulation (like pandas)",
  "readme": "https://nl253.github.io/DataFrame/index.html",
  "homepage": "https://github.com/nl253/DataFrame",
  "keywords": [
    "table",
    "dataframe",
    "array",
    "typed",
    "utilities",
    "machine-learning",
    "ML",
    "ml",
    "math",
    "stats",
    "statistics"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl253/DataFrame"
  },
  "bugs": {
    "email": "norbertlogiewa96@gmail.com",
    "url": "https://github.com/nl253/DataFrame/issues"
  },
  "author": "Norbert Logiewa <norbertlogiewa96@gmail.com>",
  "maintainers": [
    {
      "email": "norbertlogiewa96@gmail.com",
      "name": "Norbert Logiewa"
    }
  ],
  "contributors": [
    {
      "email": "norbertlogiewa96@gmail.com",
      "name": "Norber Logiewa",
      "url": "https://github.com/nl253"
    }
  ],
  "engines": {
    "node": ">=10.15.1"
  },
  "engineStrict": true,
  "browserslist": [],
  "dependencies": {
    "csv": "^5.3.0",
    "csv-parse": "^4.8.2",
    "sync-request": "^6.1.0",
    "winston": "^3.2.1",
    "csv-stringify": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jsdoc": "3.6.3",
    "jsonlint": "^1.6.3",
    "license-checker": "^25.0.1",
    "markdownlint": "^0.18.0",
    "markdownlint-cli": "^0.19.0",
    "remark": "^11.0.2",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-consistent": "^2.0.3",
    "remark-preset-lint-markdown-style-guide": "^2.1.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "retire": "^2.0.3",
    "write-good": "^1.0.2"
  },
  "main": "./DataFrame.js",
  "directories": {
    "example": "./examples",
    "doc": "./docs",
    "lib": "./"
  },
  "files": [
    "README.md",
    "*.js",
    "datasets/*.csv"
  ],
  "private": false,
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-preset-lint-markdown-style-guide",
      "remark-preset-lint-consistent"
    ]
  },
  "scripts": {
    "test": "npx jest --ci",
    "doc": "bash -c \"echo -e 'GENERATING DOCS\\n' && rm -rf ./docs && npx jsdoc --pedantic -d ./docs -R ./README.md ./*.js\"",
    "doc:open": "bash -c \"npm run doc && $BROWSER ./docs/index.html\"",
    "doc:publish": "bash -c \"echo -e 'PUBLISHING DOCS\\n' && npm run doc && git add ./docs && git commit -am 'Update docs' -n && git push\"",
    "loc": "bash -c \"tokei -- $(git ls-files | grep -v docs | grep -v bin)\"",
    "lint": "bash -c \"npm run lint:code && npm run lint:doc && npm run lint:deps\"",
    "lint:code": "bash -c \"npx eslint *.js\"",
    "lint:doc": "bash -c \"npm run lint:doc:markdown-lint && npm run lint:doc:write-good && npm run lint:doc:remark\"",
    "lint:doc:write-good": "bash -c \"npx write-good --so --weasel --tooWordy --cliches --thereIs *.md || echo ''\"",
    "lint:doc:markdown-lint": "bash -c \"npx markdownlint *.md\"",
    "lint:doc:remark": "bash -c \"npx remark -u lint *.md\"",
    "lint:deps": "bash -c \"npm run lint:deps:license && npm run lint:deps:security\"",
    "lint:deps:security": "npx retire --js",
    "lint:deps:license": "npx license-checker --summary",
    "prepublishOnly": "npm run doc:publish"
  }
}
